import string
Password = input("Enter your password:")
for i in Password:
    if len(Password) >= 12:
        print("strong password")
        break
    elif len(Password) < 12:
            print("weak password") 
            break
for p in Password:   
 UpperChar= p.isupper()
 LowerChar = p.islower()
 NumChar = p.isdigit()
 special_characters = "!@#$%^&*()-+?_=,<>/"
 if p.isupper():
            UpperChar += 1
            
            if p.islower():
             LowerChar += 1
             
if p.isdigit(): 
                NumChar += 1
special_characters = "!@#$%^&*()-+?_=,<>/"
s=input()


if any(p in special_characters for p in Password):
    special_characters += 1
else:
    
      print("Missing Special Characters")



 